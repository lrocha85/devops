pipeline {
  stages {
    stage("conexao"){
      steps {
        git branch: 'main', url: 'https://github.com/lrocha85/devops.git'
      }
    }
  }
  stage('Initialize'){
        def dockerHome = tool 'myDocker'
        env.PATH = "${dockerHome}/bin:${env.PATH}"
  }
  agent{
      docker{
        image 'lrocha85/testeapp:1.1'
      }
  }
}